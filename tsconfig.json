{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "types":[
      "node", 
      "jest", 
      "playwright-core"
    ],
    "esModuleInterop": true,
    "target": "es2016",
    "lib": ["DOM", "ES2016"],
    "forceConsistentCasingInFileNames": true, 
    "strict": true, 
    "skipLibCheck": true
  },
  "ts-node": {"esm": false}
}